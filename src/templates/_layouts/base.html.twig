<!doctype html>
<html lang="{{ locale|split('_')[0] }}">
<head>
    <meta charset="utf-8" />
    <link rel="dns-prefetch" href="//fonts.googleapis.com" />
    <link rel="dns-prefetch" href="//themes.googleusercontent.com" />
    <link rel="dns-prefetch" href="https://www.google-analytics.com" />

    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <title>{% filter striptags|trim %}{% block title %}Bootstrap UI Demo{% endblock %}{% endfilter %}</title>
    <meta name="description" content="Bootstrap UI Design Pattern Library" />
    <meta name="author" content="VisionApps, s.r.o. [www.visionapps.cz]" />

    {% block styles %}
        <link rel="stylesheet" href="//fonts.googleapis.com/css?family=Titillium+Web:400,300,700&amp;subset=latin,latin-ext" type="text/css" />
        <link rel="stylesheet" href="{{ asset('css/main.min.css') }}" type="text/css" />
    {% endblock %}

    <link rel="icon" href="data:image/png;base64," />

    {% if tracking.google_analytics is not empty %}
        <script src="{{ asset(tracking.google_analytics) }}"></script>
    {% endif %}
</head>
<body class="site{% if bodyClass is defined %} {{ bodyClass }}{% endif %}">

{% block sublayout %}{% endblock %}

{% block scripts %}{% endblock %}

</body>
</html>
